<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ExpressoOLabs</title>
    <environment names="Development">
        <link href="~/css/bootstrap.css" rel="stylesheet" />
        <link href="~/css/bootstrap-responsive.css" rel="stylesheet" />
        <link href="~/css/docs.css" rel="stylesheet" />
        <link href="~/css/prettyPhoto.css" rel="stylesheet" />
        <link href="~/css/prettify.css" rel="stylesheet" />
        <link href="~/css/flexslider.css" rel="stylesheet" />
        <link href="~/css/font-awesome.css" rel="stylesheet" />
        <link href="~/css/refineslide.css" rel="stylesheet" />
        <link href="~/css/animate.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
        <link href="~/css/default.css" rel="stylesheet" />
        <link href="~/css/overwrite.css" rel="stylesheet" />
        <link href="~/css/headerfix.css" rel="stylesheet" />
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:400italic,400,600,700" rel="stylesheet">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/ico/apple-touch-icon-114-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/ico/apple-touch-icon-144-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/ico/apple-touch-icon-57-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="~/ico/apple-touch-icon-72-precomposed.png">
    </environment>
    <environment names="Staging,Production">
        <link href="~/css/bootstrap.css" rel="stylesheet" />
        <link href="~/css/bootstrap-responsive.css" rel="stylesheet" />
        <link href="~/css/docs.css" rel="stylesheet" />
        <link href="~/css/prettyPhoto.css" rel="stylesheet" />
        <link href="~/css/prettify.css" rel="stylesheet" />
        <link href="~/css/flexslider.css" rel="stylesheet" />
        <link href="~/css/font-awesome.css" rel="stylesheet" />
        <link href="~/css/refineslide.css" rel="stylesheet" />
        <link href="~/css/animate.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
        <link href="~/css/default.css" rel="stylesheet" />
        <link href="~/css/overwrite.css" rel="stylesheet" />
        <link href="~/css/headerfix.css" rel="stylesheet" />
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:400italic,400,600,700" rel="stylesheet">
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/ico/apple-touch-icon-114-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/ico/apple-touch-icon-144-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/ico/apple-touch-icon-57-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="~/ico/apple-touch-icon-72-precomposed.png">
    </environment>
</head>
<body>
    <div class="cbp-af-header">
        <div class="cbp-af-inner">
            <div class="container">
                <div class="row">
                    <div class="span4">
                        <!-- logo -->
                        <div class="logo" style="font-size: 7px !important">
                            <h1>
                                <a href='@Url.Action("Index", "Home")'>
                                    <strong style="color: #5AD0B8 !important;">ExpressoO</strong> Labs
                                </a>
                            </h1>
                        </div>
                        <!-- end logo -->
                    </div>
                    <div class="span8">
                        <!-- top menu -->
                        <div class="navbar">
                            <div class="navbar-inner">
                                <nav>
                                    <ul class="nav topnav" id="menu">
                                        <li class="@(ViewBag.Current == "Home" ? "active" : "") "><a href='@Url.Action("Index", "Home")'>Home</a></li>
                                        <li class="@(ViewBag.Current == "About" ? "active" : "") "><a href='@Url.Action("About", "Home")'>About us</a></li>
                                        <li class="@(ViewBag.Current == "Services" ? "active" : "") dropdown">
                                            <a href='@Url.Action("DigitalMarkerting", "Services")'>Services</a>
                                            <ul class="dropdown-menu">
                                                <li class="@(ViewBag.Current == "Services" ? "active" : "")">
                                                    <a href='@Url.Action("DigitalMarkerting", "Services")'>Digital Marketing</a>
                                                    <ul class="dropdown-menu sub-menu" style="margin-top: -1px !important">
                                                        <li><a href='@Url.Action("SeoServicesinHyderabad", "Services")'>SEO Serivces</a></li>
                                                        <li><a href='@Url.Action("SocialmediaManagementCompany", "Services")'>SMM Services</a></li>
                                                        <li><a href='@Url.Action("PpcManagementServices", "Services")'>PPC Services</a></li>
                                                        <li><a href='@Url.Action("OnlineReputationManagementServices", "Services")'>ORM Services</a></li>
                                                        <li><a href='@Url.Action("CRMServices", "Services")'>CRM Services</a></li>
                                                        <li><a href='@Url.Action("FacebookMarketing", "Services")'>Facebook Marketing</a></li>
                                                    </ul>
                                                </li>
                                                <li class="@(ViewBag.Current == "Services" ? "active" : "")"><a href='@Url.Action("WebDesignAndDev", "Services")'>Web Design & Development</a></li>
                                                <li class="@(ViewBag.Current == "Services" ? "active" : "")"><a href='@Url.Action("DigitalMediaServices", "Services")'>Digital Movie Promotions</a></li>
                                                <li class="@(ViewBag.Current == "Services" ? "active" : "")"><a href='@Url.Action("StaffingSolutions", "Services")'>Staffing Solutions</a></li>
                                            </ul>
                                        </li>
                                        <li class="@(ViewBag.Current == "Products" ? "active" : "") dropdown">
                                            <a href='@Url.Action("ProductExpressoO", "Products")'>Products</a>
                                            <ul class="dropdown-menu">
                                                <li class="@(ViewBag.Current == "Products" ? "active" : "")"><a href='@Url.Action("ProductExpressoO", "Products")'>ExpressoO Hub</a></li>
                                                <li class="@(ViewBag.Current == "Products" ? "active" : "")"><a href='@Url.Action("ProductHireSteer", "Products")'>Hire Steer</a></li>
                                                <li class="@(ViewBag.Current == "Products" ? "active" : "")"><a href='@Url.Action("ProductMachinoo", "Products")'>Machinoo</a></li>
                                            </ul>
                                        </li>
                                        <li class="@(ViewBag.Current == "Blog" ? "active" : "") ">
                                            <a href='@Url.Action("Blog", "Home")'>Blog</a>
                                        </li>
                                        <li class="@(ViewBag.Current == "Careers" ? "active" : "") ">
                                            <a href='@Url.Action("Careers", "Home")'>Careers</a>
                                        </li>
                                        <li class="@(ViewBag.Current == "Contact" ? "active" : "") ">
                                            <a href='@Url.Action("Contact", "Home")'>Contact</a>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                        <!-- end menu -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div style="margin-top:-10px !important">
        @RenderBody()
        <hr />
        <footer style="background-color: #3a3a3a !important">
            <div class="container">
                <div class="row">
                    <div class="span3">
                        <div class="widget">
                            <!-- logo -->
                            <div class="footerlogo">
                                <h6><a href='@Url.Action("Index", "Home")'>ExpressoO Labs</a></h6>
                                <!-- <img src="../images/logo.png" alt="" /> -->
                            </div>
                            <!-- end logo -->
                            <address>
                                <strong>ExpressoO Labs</strong><br>
                                7-1-32/5/4, Jamuna Tirth, Ameerpet, Hyderabad, Telangana 500016<br>
                                <abbr title="Phone">P:</abbr> (040) 485-39966
                                <abbr title="Mobile">M:</abbr> (949) 226-1712
                            </address>
                        </div>
                    </div>
                    <div class="span3">
                        <div class="widget">
                            <h5>Browse pages</h5>
                            <ul class="list list-ok">
                                <li><a href='@Url.Action("DigitalMarkerting", "Services")'>Digital Marketing Services</a></li>
                                <li><a href='@Url.Action("StaffingSolutions", "Services")'>Staffing solutions</a></li>
                                <li><a href='@Url.Action("WebDesignAndDev", "Services")'>Web Design & Development</a></li>
                                <li><a href='@Url.Action("Careers", "Home")'>We're hiring</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="span3">
                        <div class="widget">
                            <h5>Flickr photostream</h5>
                            <div class="flickr_badge">
                                <script type="text/javascript" src="http://www.flickr.com/badge_code_v2.gne?count=8&amp;display=random&amp;size=s&amp;layout=x&amp;source=user&amp;user=34178660@N03"></script>
                            </div>
                            <div class="clear"></div>
                        </div>
                    </div>
                    <div class="span3">
                        <div class="widget">
                            <h5>Keep updated</h5>
                            <p>
                                Enter your email to subcribe newsletter
                            </p>
                            <form>
                                <div class="input-append">
                                    <input class="span2" id="appendedInputButton" type="text">
                                    <button class="btn btn-color" type="submit">Subscribe</button>
                                </div>
                            </form>
                            <ul class="social-network">
                                <li><a href="#"><i class="icon-bg-light icon-facebook icon-circled icon-1x"></i></a></li>
                                <li><a href="#" title="Twitter"><i class="icon-bg-light icon-twitter icon-circled icon-1x"></i></a></li>
                                <li><a href="#" title="Linkedin"><i class="icon-bg-light icon-linkedin icon-circled icon-1x"></i></a></li>
                                <li><a href="#" title="Pinterest"><i class="icon-bg-light icon-pinterest icon-circled icon-1x"></i></a></li>
                                <li><a href="#" title="Google plus"><i class="icon-bg-light icon-google-plus icon-circled icon-1x"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="subfooter">
                <div class="container">
                    <div class="row">
                        <div class="span6">
                            <p>
                                &copy; Plato - All right reserved
                            </p>
                        </div>
                        <div class="span6">
                            <div class="pull-right">
                                <div class="credits">
                                    <!--
                                      All the links in the footer should remain intact.
                                      You can delete the links only if you purchased the pro version.
                                      Licensing information: https://bootstrapmade.com/license/
                                      Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/buy/?theme=Plato
                                    -->
                                    Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <environment names="Development">
        <script src="~/js/jquery.js"></script>
        <script src="~/js/modernizr.js"></script>
        <script src="~/js/jquery.easing.1.3.js"></script>
        <script src="~/js/prettify.js"></script>
        <script src="~/js/bootstrap.js"></script>
        <script src="~/js/jquery.prettyPhoto.js"></script>
        <script src="~/js/jquery.quicksand.js"></script>
        <script src="~/js/setting.js"></script>
        <script src="~/js/jquery-hover-effect.js"></script>
        <script src="~/js/jquery.flexslider.js"></script>
        <script src="~/js/classie.js"></script>
        <script src="~/js/cbpAnimatedHeader.min.js"></script>
        <script src="~/js/jquery.refineslide.js"></script>
        <script src="~/js/jquery.ui.totop.js"></script>
        <script src="~/js/custom.js"></script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyARWirOJzOUGacWZmnh-5plM9E5QeIpOKg"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/prettyPhoto/3.1.6/js/jquery.prettyPhoto.min.js"></script>
        <script src="~/js/jquery.quicksand.min.js"></script>
        <script src="~/js/settings.min.js"></script>
        <script src="~/js/jquery-hover-effect.min.js"></script>
        <script src="~/js/jquery.flexslider.min.js"></script>
        <script src="~/js/classie.min.js"></script>
        <script src="~/js/cbpAnimatedHeader.min.js"></script>
        <script src="~/js/jquery.refineslide.min.js"></script>
        <script src="~/js/jquery.ui.totop.min.js"></script>
        <script src="~/js/custom.min.js"></script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyARWirOJzOUGacWZmnh-5plM9E5QeIpOKg"></script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
